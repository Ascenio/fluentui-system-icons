{
  "name": "@fluentui/svg-icons",
  "version": "1.1.121",
  "description": "Fluent System Icons are a collection of familiar, friendly, and modern icons from Microsoft.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/fluentui-system-icons.git"
  },
  "scripts": {
    "clean": "shx rm -rf ./icons",
    "copy": "node ../../importer/generate.js --source=../../assets --dest=./icons --extension=svg",
    "rename": "renamer --find ic_fluent_ ./icons/*",
    "optimize": "svgo --folder=./icons --precision=2 --disable=removeViewBox,mergePaths",
    "unfill": "node unfill.js --path=./icons/",
    "unfillUbuntu": "find ./icons -type f -name \"*.svg\" -exec sed -i.bak 's/fill=\"none\"/class=\"fluent_system_icon\"/g' {} \\; && find ./icons -type f -name \"*.bak\" -delete",
    "fillPrimary": "find ./icons -type f -name \"*.svg\" -exec sed -i.bak 's/fill=\"#212121\"/style=\"{fill_primary}\"/g' {} \\; && find ./icons -type f -name \"*.bak\" -delete",
    "fillSecondary": "find ./icons -type f -name \"*.svg\" -exec sed -i.bak 's/fill=\"#6E6E6E\"/style=\"{fill_secondary}\"/g' {} \\; && find ./icons -type f -name \"*.bak\" -delete",
    "fillTertiary": "find ./icons -type f -name \"*.svg\" -exec sed -i.bak 's/fill=\"#ACACAC\"/style=\"{fill_tertiary}\"/g' {} \\; && find ./icons -type f -name \"*.bak\" -delete",
    "build": "npm run copy && npm run rename && npm run optimize && npm run unfill",
    "buildColor": "npm run copy && npm run rename && npm run optimize && npm run unfillUbuntu && npm run fillPrimary && npm run fillSecondary && npm run fillTertiary",
    "testColor": "npm run unfillUbuntu && npm run fillPrimary && npm run fillSecondary && npm run fillTertiary"
  },
  "devDependencies": {
    "renamer": "^2.0.1",
    "replace": "^1.2.0",
    "shx": "^0.3.2",
    "svgo": "^1.3.2",
    "yargs": "^14.0.0"
  }
}
